{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "public/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
